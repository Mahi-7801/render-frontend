{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "elasticbeanstalk:*",
                "s3:*",
                "ec2:*",
                "iam:PassRole",
                "iam:GetRole",
                "cloudwatch:*",
                "autoscaling:*",
                "rds:*",
                "sqs:*",
                "sns:*",
                "logs:*",
                "cloudformation:*"
            ],
            "Resource": "*"
        }
    ]
} 